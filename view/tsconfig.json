{
  "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "strict": false,
      "moduleResolution": "node",
      "importHelpers": true,
      "esModuleInterop": true,
      "noImplicitAny": false,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "types": [
          "vuetify",
          "webpack-env"
      ],
      "paths": {
          "@/*": [
              "src/*"
          ]
      },
      "lib": [
          "esnext",
          "dom",
          "dom.iterable",
          "scripthost"
      ]
  },
  "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.vue"
  ],
  "exclude": [
      "node_modules"
  ]
}
